#1 
class person: 
    def __init__(self,fname, lname): 
        self.fname=fname 
        self.lname=lname 
p1=person("Harsh","Agarwal") 
print(p1.fname,p1.lname) 

 

 

 

#2 
class Dog: 
    kind="cannie" 
    def __init__(self,name): 
        self.name = name 
d=Dog("fibo") 
e=Dog("Buddy") 
print(d.kind) 
print(e.kind) 
print(d.name) 
print(e.name) 
